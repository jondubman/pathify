{
  "files.exclude": {
    "**/.flowconfig": true,
    "**/app/android/**": true,
    "**/app/ios/Pathify.xcodeproj": false,
    "**/app/shared": true,
    "**/app/shared/**": true,
    "**/package-lock.json": true,
    "**/server/src/shared": true,
    "**/server/src/shared/**": true,
    "**/yarn.lock": true,
    "app/shared/**": true,
    "server/src/shared/**": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": false,
    "**/.factorypath": true
  },
  "workbench.colorTheme": "Tomorrow Night Blue",
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#ca1f1f",
    "editor.findMatchBackground": "#6122d6",
    "editor.findMatchHighlightBackground": "#0f8f3392",
  },
  "editor.occurrencesHighlight": false,
  "editor.selectionHighlight": false,
  "search.quickOpen.includeHistory": false,
  "typescript.suggest.autoImports": false,
  "search.exclude": {
    "**/app/shared": true,
    "**/server/src/shared": true
  }
}