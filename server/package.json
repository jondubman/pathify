{
  "name": "pathify-server",
  "version": "1.0.0",
  "description": "Pathify server",
  "scripts": {
    "clean": "rm -rf dist; mkdir dist 2>/dev/null",
    "clean-reset": " yarn prisma-force-reset; yarn clean",
    "build": "yarn clean; cp -R prisma/generated/prisma-client src; tsc; cp -R src/config dist; echo 'built to /server/dist'; ls -l dist; yarn test",
    "build-reset": "yarn build; yarn prisma-force-reset",
    "launch": "NODE_ENV=development node dist/server.js",
    "test": "NODE_ENV=development mocha dist/test/*.js",
    "prisma-generate": "pushd .; cd prisma; prisma deploy; prisma generate; popd",
    "prisma-reset": "pushd .; cd prisma; prisma reset; prisma deploy; prisma generate; prisma seed; popd",
    "prisma-force-reset": "pushd .; cd prisma; prisma reset -f; prisma deploy; prisma generate; prisma seed; popd",
    "start": "echo \"launching nodemon, watching src...\"; NODE_ENV=development nodemon --watch src -e ts,json5 --exec 'echo \"rebuilding server...\"; yarn build; echo \"running tests...\"; echo \"relaunching server...\"; yarn launch'"
  },
  "author": "Jonathan Dubman",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.1.3",
    "@types/chai-as-promised": "^7.1.0",
    "@types/config": "^0.0.34",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/microtime": "^2.1.0",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.6",
    "@types/mongodb": "^3.1.26",
    "@types/node": "^12.0.0",
    "@types/node-fetch": "^2.3.3",
    "@types/xml2js": "^0.4.4",
    "aws-sdk": "^2.453.0",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "bunyan-middleware": "^0.8.0",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "colors": "^1.3.0",
    "config": "^3.1.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.15.3",
    "file-loader": "^3.0.1",
    "geolib": "^2.0.24",
    "graphql": "^14.3.0",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "microtime": "^3.0.0",
    "minimist": "^1.2.0",
    "mocha": "^6.1.4",
    "module-alias": "^2.2.0",
    "mongodb": "^3.2.4",
    "node-fetch": "^2.5.0",
    "nodemon": "^1.19.0",
    "prisma-client-lib": "^1.30.4",
    "sleep-promise": "^8.0.1",
    "spdy": "^4.0.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "url-loader": "^1.0.1",
    "uuid": "^3.2.1",
    "winston": "^3.2.1",
    "xml2js": "^0.4.19"
  },
  "_moduleAliases": {
    "lib": "dist/lib",
    "routers": "dist/routers",
    "prisma-client": "dist/prisma-client",
    "shared": "dist/shared",
    "test": "dist/test"
  }
}
