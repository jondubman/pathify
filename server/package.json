{
  "name": "pathify-server",
  "version": "1.0.0",
  "description": "Pathify server",
  "scripts": {
    "clean": "rm -rf dist; mkdir -p dist; mkdir -p logs",
    "build": "yarn clean; cd ../shared; yarn share; cd ../server; tsc; cp -R src/config dist; cp -R src/secrets dist; echo 'built to /server/dist'",
    "build-test": "yarn build; yarn test",
    "launch": "NODE_ENV=development node dist/pathify.js",
    "log-trace": "tail -f logs/complete.log | bunyan -l trace",
    "log-debug": "tail -f logs/complete.log | bunyan -l debug",
    "log": "tail -f logs/complete.log | bunyan -l debug",
    "log-info": "tail -f logs/complete.log | bunyan -l info",
    "log-warn": "tail -f logs/complete.log | bunyan -l warn",
    "test": "NODE_ENV=development mocha dist/test/*.js",
    "restart": "yarn build; yarn launch",
    "start": "yarn; yarn build; yarn launch",
    "start-test": "yarn; yarn build-test; yarn launch"
  },
  "author": "Jonathan Dubman",
  "license": "ISC",
  "dependencies": {
    "@turf/distance": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@turf/turf": "^5.1.6",
    "@types/body-parser": "^1.19.0",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/microtime": "^2.1.0",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^8.0.3",
    "@types/mongodb": "^3.5.26",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/xml2js": "^0.4.5",
    "aws-sdk": "^2.739.0",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.14",
    "bunyan-middleware": "^1.0.0",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "colors": "^1.4.0",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "geolib": "^3.3.1",
    "graphql": "^15.3.0",
    "helmet": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "microtime": "^3.0.0",
    "minimist": "^1.2.5",
    "mocha": "^8.1.1",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.4",
    "sleep-promise": "^8.0.1",
    "spdy": "^4.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "util": "^0.12.3",
    "uuid": "^3.4.0",
    "winston": "^3.3.3",
    "xml2js": "^0.4.23"
  },
  "_moduleAliases": {
    "lib": "dist/lib",
    "routers": "dist/routers",
    "shared": "dist/shared",
    "test": "dist/test"
  }
}
